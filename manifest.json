{
    "name": "Kriptou",
    "description": "Cryptez vos messages",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
      "manifest_version": 2,
    "icons": {
        "128": "icon.png"
    }, 
    "permissions":[
    "storage", 
    "clipboardWrite",
     "webRequest",
       "*://*/*"
    ],
"browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": [ "scripts/jquery-1.11.0.min.js", "scripts/script.js"]
    }
  ]
}
